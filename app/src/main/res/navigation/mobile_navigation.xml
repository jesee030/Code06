<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.code06.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dynamic"
        android:name="com.example.code06.ui.dashboard.DashboardFragment"
        android:label="@string/title_dynamic"
        tools:layout="@layout/fragment_dashboard" />



    <fragment
        android:id="@+id/navigation_person"
        android:name="com.example.code06.ui.person.PersonFragment"
        android:label="@string/title_person"
        tools:layout="@layout/fragment_home" />

</navigation>